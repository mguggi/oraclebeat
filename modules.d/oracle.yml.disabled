- module: oracle
  period: 30s
  metricsets:
    - session
    - sgastats
  # List of Oracle connection strings
  hosts: ["oracle://system:Login4Me@localhost:1521/XE"]

- module: oracle
  period: 1m
  metricsets:
    - sqlstats
    - sysmetric
    - sessmetric
    - tablespace

  # List of Oracle connection strings
  hosts: ["oracle://system:Login4Me@localhost:1521/XE"]
